<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="#1f1f1f">


    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/matchedname"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:gravity="center"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:id="@+id/matchednumber"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:gravity="center"
        android:layout_below="@+id/matchedname"/>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:weightSum="6"
            android:layout_marginTop="100dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="horizontal"
                android:weightSum="1">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:id="@+id/number"
                    android:layout_weight="0.9"
                    android:textAlignment="center"
                    android:textColor="#FFFFFF"
                    android:textSize="30sp"
                    android:gravity="center"
                    />

                <ImageView
                    android:id="@+id/cancel"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:layout_gravity="right|center_vertical"
                    android:background="@drawable/delete"
                    android:layout_weight="0.1"
                    />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="5"
                android:weightSum="5"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:weightSum="3">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:id="@+id/one"
                        android:layout_weight="1"
                        android:text="1"
                        android:textColor="#FFFFFF"
                        android:textSize="50sp"
                        android:gravity="center"
                        />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:id="@+id/two"
                        android:layout_weight="1"
                        android:text="2"
                        android:textColor="#FFFFFF"
                        android:textSize="50sp"
                        android:gravity="center"
                        />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:id="@+id/three"
                        android:layout_weight="1"
                        android:text="3"
                        android:textColor="#FFFFFF"
                        android:textSize="50sp"
                        android:gravity="center"
                        />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:weightSum="3">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:id="@+id/four"
                        android:layout_weight="1"
                        android:text="4"
                        android:textColor="#FFFFFF"
                        android:textSize="50sp"
                        android:gravity="center"
                        />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:id="@+id/five"
                        android:layout_weight="1"
                        android:text="5"
                        android:textColor="#FFFFFF"
                        android:textSize="50sp"
                        android:gravity="center"
                        />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:id="@+id/six"
                        android:layout_weight="1"
                        android:text="6"
                        android:textColor="#FFFFFF"
                        android:textSize="50sp"
                        android:gravity="center"
                        />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:weightSum="3">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:id="@+id/seven"
                        android:layout_weight="1"
                        android:text="7"
                        android:textColor="#FFFFFF"
                        android:textSize="50sp"
                        android:gravity="center"
                        />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:id="@+id/eight"
                        android:layout_weight="1"
                        android:text="8"
                        android:textColor="#FFFFFF"
                        android:textSize="50sp"
                        android:gravity="center"
                        />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:id="@+id/nine"
                        android:layout_weight="1"
                        android:text="9"
                        android:textColor="#FFFFFF"
                        android:textSize="50sp"
                        android:gravity="center"
                        />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:weightSum="3">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:id="@+id/star"
                        android:layout_weight="1"
                        android:text="*"
                        android:textColor="#FFFFFF"
                        android:textSize="50sp"
                        android:gravity="center"/>

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:id="@+id/zero"
                        android:layout_weight="1"
                        android:text="0"
                        android:textColor="#FFFFFF"
                        android:textSize="50sp"
                        android:gravity="center"
                        />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:id="@+id/hatch"
                        android:layout_weight="1"
                        android:text="#"
                        android:textColor="#FFFFFF"
                        android:textSize="50sp"
                        android:gravity="center"
                        />




                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:orientation="horizontal"
                    android:weightSum="3">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:id="@+id/msg"
                        android:layout_weight="1"
                        android:background="@drawable/msg_three"
                        android:pointerIcon="grab"
                        android:gravity="center"
                        android:padding="20dp"
                        android:layout_margin="20dp"/>

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:id="@+id/call"
                        android:layout_weight="1"
                        android:background="@drawable/phone_call"
                        android:gravity="center"
                        android:layout_margin="20dp"
                        />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:id="@+id/whatsapp"
                        android:layout_weight="1"
                        android:background="@drawable/whatsapp_text"
                        android:gravity="center"
                        android:layout_margin="15dp"
                        />




                </LinearLayout>

            </LinearLayout>

        </LinearLayout>





</RelativeLayout>